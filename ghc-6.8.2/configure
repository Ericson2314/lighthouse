# include <inttypes.h>
#else
# if HAVE_STDINT_H
#  include <stdint.h>
# endif
#endif
#if HAVE_UNISTD_H
# include <unistd.h>
#endif"

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS CONFIGURE_ARGS ProjectName ProjectVersion ProjectVersionInt ProjectPatchLevel release GMP_INCLUDE_DIRS GMP_LIB_DIRS build build_cpu build_vendor build_os host host_cpu host_vendor host_os target target_cpu target_vendor target_os BuildPlatform HostPlatform TargetPlatform HostPlatform_CPP BuildPlatform_CPP TargetPlatform_CPP HostArch_CPP BuildArch_CPP TargetArch_CPP HostOS_CPP HostOS_Full BuildOS_CPP TargetOS_CPP HostVendor_CPP BuildVendor_CPP TargetVendor_CPP exeext soext GHC WithGhc WithHc GhcVersion GhcMajVersion GhcMinVersion GhcPatchLevel ghc_ge_601 ghc_ge_602 ghc_ge_603 ghc_ge_605 ghc_ge_607 GhcPkgCmd GhcHasReadline NHC HBC WhatGccIsCalled fp_prog_ld_raw LdCmd BootingFromHc BootingFromUnregisterisedHc hardtop GhcLibsWithObjectIO DotnetSupport GhcLibsWithUnix PerlCmd PythonCmd CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT HaveGcc GccVersion GccExtraViaCOpts CPP SRC_CC_OPTS ContextDiffCmd fp_prog_find FindCmd fp_prog_sort SortCmd INSTALL_PROGRAM INSTALL_SCRIPT INSTALL_DATA fp_prog_ar_raw ArCmd RANLIB ac_ct_RANLIB ArSupportsInput LN_S SedCmd TimeCmd TarCmd HSCOLOUR XmllintCmd XsltprocCmd DIR_DOCBOOK_XSL FopCmd1 FopCmd2 FopCmd XmltexCmd DvipsCmd PdfxmltexCmd GreenCardCmd GreenCardVersion HappyCmd HappyVersion HaddockCmd AlexCmd AlexVersion EGREP GhcLibsWithReadline HaveLibGmp LibGmp HaveFrameworkGMP HaveLibMingwEx LIBM HaveLibDL ALLOCA LeadingUnderscore LdXFlag LdIsGNULd GTK_CONFIG HavePapiLib HavePapiHeader HavePapi LIBOBJS LTLIBOBJS'
ac_subst_files=''

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
cache_file=/dev/null
exec_prefix=NONE
no_create=
